function fnCheckMandatoryLogin(cssErrorClass, cssNormalCssClass, ControlID) {
    var txtUserId = document.getElementById(ControlID + "_txtUserName");
    var lblError = document.getElementById(ControlID + "_lblLoginError");
    var txtPassword = document.getElementById(ControlID + "_txtPassword");
    var errorMandatory = Messages["AllMn"];
    if (fnCheckNull(txtUserId)) {
        errorMandatory = Messages["UserNameMn"];
        txtUserId.focus();
        lblError.innerHTML = errorMandatory;
        return false
    }
    if (fnCheckNull(txtPassword)) {
        errorMandatory = Messages["PasswordMn"];
        txtPassword.focus();
        lblError.innerHTML = errorMandatory;
        return false
    }
    return true
}
function fnCheckNull(txtControl) {
    var isNull = false;
    if (fn_trim(txtControl.value) == "") {
        isNull = true
    } else {
        txtControl.style.borderStyle = "inset";
        txtControl.style.borderColor = "";
        isNull = false
    }
    return isNull
}
function fn_trim(str) {
    return str.replace("/^\s*|\s*$/g", "")
}
function fnDefaultClick(element1) {
    var btn = document.getElementById(element1);
    if (event.keyCode == 13) 
    {       
        if (document.activeElement.tagName != "A" && document.activeElement.type != "checkbox") {
            btn.click();
            return false
        }
        if (document.activeElement.type == "checkbox") {
            if (document.activeElement.checked) {
                document.activeElement.checked = false
            } else {
                document.activeElement.checked = true
            }
            return false
        }
    }
}