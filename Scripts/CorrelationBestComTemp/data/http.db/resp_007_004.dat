        var col = new String();
        var x = 1; var y;
        var f = 1; var t;
        function blink() {
            if (x % 2) {
                col = "rgb(46,139,87)";
                
            }
            else {
                //col = "rgb(144,238,144)";
                col = "rgb(122,170,27)";
            }
            if (f % 2) {
                colf = "rgb(122,0,40)";

            }
            else {
                //col = "rgb(144,238,144)";
                colf = "rgb(255,64,31)";
            }  
            var QuickPayment = document.getElementById("ctl00_ctl00_MasterPageContentPlaceHolder_MasterPageContentPlaceHolder_linkQuickPayment");

            if (QuickPayment != null) {
                QuickPayment.style.color = col; x++;
            }
            var FTNewconnection = document.getElementById("ctl00_ctl00_MasterPageContentPlaceHolder_MasterPageContentPlaceHolder_LinkFastTrackNewconnection");

            if (FTNewconnection != null) {
                FTNewconnection.style.color = colf; f++;
            }

            if (x > 2) { x = 1 };
            if (f > 2) { f = 1 };
            setTimeout("blink()", 500);
        }               

    </script>

