
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">

<html>

<head>
	<title>Internet Client Log On</title>
	<meta http-equiv="cache-control" content="no cache">
	<meta http-equiv="pragma" content="no cache">
	<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
	<link rel="stylesheet" type="text/css" href="style/loginstyle.css">
	<meta http-equiv="Content-Style-Type" content="text/css">
	<script src="js/login.js" language="javascript" type="text/javascript"></script>
	<script type="text/javascript" language="JavaScript">
	function FormFocus() {
		document.frmLogin.df_username.focus();
	}

	function showError(nErrorNo) {
		switch(nErrorNo)
		{
			case 1:
				var lcMessage = 'You have not entered the correct username or password.'
				break;
			case 5:
				var lcMessage = 'Failed to display SMS page.'
				break;
			case 6:
				var lcMessage = 'Failed to send SMS with one-time password, validate your phone number with customer service.'
				break;
			case 7:
				var lcMessage = 'Could not load your phone number for SMS password, contact customer service.'
				break;
			case 8:
				var lcMessage = 'Incorrect SMS one-time password.'
				break;
			case 100: 
				var lcMessage = 'Unknown error in BankID authentication.'
				break;
			case 101: 
				var lcMessage = 'You are not a customer with us.'
				break;
			case 102: 
				var lcMessage = 'Your BankID is not valid.'
				break;
			case 3005:
				var lcMessage = 'Invalid human verification.'
				break;
			case 3006:
				var lcMessage = 'Invalid human verification.'
				break;
			default:
				var lcMessage = 'You have not entered the correct username or password.'
				break;
		}
			
		lcMessage = lcMessage.replace(/(<([^>]+)>)/ig,""); 
		alert(lcMessage);
	}
	</script>
</head>

<body onLoad="FormFocus();">
<!-- Table that fills the whole screen used to center the login window -->
<table border="0" cellpadding="0" cellspacing="0" width="100%" height="100%">
<tr><td align="center">
	<!-- Table containing form data starts here ! -->
	<form method="post" action="login.mws" name="frmLogin">
	<input type="hidden" name="action" value="login">
  <input type="hidden" name="logintoken" value="5DS0GZWNK"> 
	<table border="0" cellpadding="0" cellspacing="0" width="319" class="textgrey">
		<tr>
			<td valign="top"><IMG src="images/login_logo.gif" border=0></td>
		</tr>
		<tr><td>
			<table border="0" cellpadding="4" cellspacing="0" width="100%" class="text">
